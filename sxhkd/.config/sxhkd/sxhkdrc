# Open terminal
super + Return
	kitty

# Open launcher
super + r
	rofi -show drun

# Open explorer
super + e
  thunar

# Reload sxhkd configuration
super + Escape
	pkill -USR1 -x sxhkd

# quit/restart bspwm
super + alt + {q,r}
	bspc {quit, wm -r}


#[FOCUS/SWAP]

# focus node
super + {_,shift +}{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}
